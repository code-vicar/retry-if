{
  "presets": [
    ["@babel/env", {
      "targets": {
        "node": "8",
        "browsers": ["last 2 versions"],
        "ie": "11"
      },
    }]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-object-rest-spread",
      { "loose": true }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        "babel-plugin-rewire"
      ]
    }
  }
}
